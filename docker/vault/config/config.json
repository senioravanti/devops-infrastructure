{
  "storage": {
    "raft": {
      "path": "/vault/file",
      "node_id": "node1"
    }
  },
  
  "listener": [
    {
      "tcp": {
        "address": "0.0.0.0:8200",
        "tls_disable": false,
        "tls_cert_file": "/etc/ssl/private/vault/fullchain.pem",
        "tls_key_file": "/etc/ssl/private/vault/privkey.pem"
      }
    }
  ],

  "api_addr": "https://127.0.0.1:8200",
  "cluster_addr": "https://127.0.0.1:8201",
  "disable_mlock": true,

  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "ui": true
}